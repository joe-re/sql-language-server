{
  "private": true,
  "name": "sql-lsp-monaco-editor-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "webpack:watch": "webpack -w",
    "copy": "mkdir -p dist && cp src/index.html dist/index.html",
    "build": "yarn tsc && webpack && yarn run copy",
    "watch": "run-p webpack:watch copy ",
    "start": "ts-node-dev --transpile-only ./src/server/server.ts",
    "migrate:postgres": "NODE_ENV=postgres sequelize-cli db:migrate",
    "migrate:mysql": "NODE_ENV=mysql sequelize-cli db:migrate",
    "migrate:sqlite": "NODE_ENV=sqlite sequelize-cli db:migrate"
  },
  "dependencies": {
    "@codingame/monaco-jsonrpc": "^0.3.1",
    "express": "^4.17.1",
    "monaco-editor-core": "^0.22.3",
    "monaco-languageclient": "^0.13.1-next.1",
    "reconnecting-websocket": "^4.4.0",
    "vscode-ws-jsonrpc": "^0.2.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^2.0.1",
    "@types/express": "^4.17.6",
    "@types/ws": "^7.2.6",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "mysql2": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0",
    "source-map-loader": "^1.0.1",
    "sqlite3": "^5.0.0",
    "style-loader": "^1.2.1",
    "svelte": "^3.24.0",
    "svelte-loader": "^2.13.6",
    "ts-loader": "^7.0.5",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^4.4.2",
    "wait-on": "^5.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
