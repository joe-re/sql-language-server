{
  "name": "sql-language-server",
  "version": "1.2.0",
  "main": "dist/src/index.js",
  "bin": {
    "sql-language-server": "./npm_bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joe-re/sql-language-server.git"
  },
  "bugs": {
    "url": "https://github.com/joe-re/sql-language-server/issues"
  },
  "keywords": [
    "sql",
    "language-server",
    "language-server-protocol",
    "lint",
    "autocompletion"
  ],
  "license": "MIT",
  "scripts": {
    "compile:cli": "esbuild bin/cli.ts --bundle --platform=node --external:pg-native --outfile=dist/cli.js",
    "compile:index": "tsc -p .",
    "clean": "rm -rf dist",
    "watch:cli": "yarn compile:cli --watch",
    "watch:index": "tsc -w",
    "test": "jest",
    "prepare-vsc-extension": "yarn clean && yarn compile:cli",
    "prepublish": "yarn clean && yarn compile:index"
  },
  "files": [
    "package.json",
    "npm_bin",
    "dist"
  ],
  "dependencies": {
    "@google-cloud/bigquery": "^5.9.0",
    "@joe-re/sql-parser": "^1.2.0",
    "@types/pg": "^7.4.10",
    "cardinal": "^2.1.1",
    "jest": "^26.0.1",
    "log4js": "^6.2.1",
    "mysql2": "^2.3.0",
    "node-ssh-forward": "^0.6.3",
    "pg": "^7.4.3",
    "sqlint": "^1.2.0",
    "sqlite3": "^4.2.0",
    "vscode-languageclient": "^6.1.3",
    "vscode-languageserver": "^8.0.0-next.3",
    "vscode-languageserver-protocol": "^3.15.3",
    "vscode-languageserver-textdocument": "^1.0.1",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/mysql": "^2.15.5",
    "@types/node": "14.0.26",
    "@types/sqlite3": "^3.1.6",
    "esbuild": "^0.13.8",
    "jest": "^26.0.1",
    "rollup": "^2.10.2",
    "ts-jest": "^26.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  }
}
