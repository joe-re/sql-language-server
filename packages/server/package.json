    "@rollup/plugin-typescript": "^4.1.1",
    "@types/jest": "^25.2.2",
    "@types/mysql": "^2.15.5",
    "@types/node": "14.0.26",
    "@types/sqlite3": "^3.1.6",
    "jest": "^26.0.1",
    "rollup": "^2.10.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2"
